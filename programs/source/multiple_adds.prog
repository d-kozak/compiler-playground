fun add_two(a,b){
    return a+b
}

fun add_three(a,b,c){
    return a+b+c
}

fun add_four(a,b,c,d){
    return a+b+c+d
}

fun mixed(a,b,c,d){
    return add_two(a,b) + add_three(a,b,c)  + add_four(a,b,c,d)
}

